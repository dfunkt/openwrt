#include "mt7621_dlink_dir-xx60-a1.dtsi"

/ {
       compatible = "dlink,dir-2150-a1", "mediatek,mt7621-soc";
       model = "D-Link DIR-2150 A1";
};

&wifi0 {
       mtd-mac-address = <&factory 0xe000>;
       mtd-mac-address-increment = <1>;
};

&wifi1 {
       mtd-mac-address = <&factory 0xe000>;
       mtd-mac-address-increment = <2>;
};
